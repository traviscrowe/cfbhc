// Can't have negative gain.
if (maxGain < 0)
    maxGain = 0;

// Add random amount of maxGain to gain.
gain += getRandomL(0, (int)maxGain);

// Break Free is the chance to break. Higher if not a check down.
int breakFree = (int) (breakStat / 2.5 + getRandomL(-5, 5) + getVariability());
if (checkDown) {
    breakFree = (int) (breakStat / 5.0 + getRandomL(-5, 5) + getVariability());
}

// Cannot be less than 15.
if (breakFree < 15) {
    breakFree = 15;
}

int numFree = 0;